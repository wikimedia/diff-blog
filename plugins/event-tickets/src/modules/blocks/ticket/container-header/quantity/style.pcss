.tribe-editor__ticket__container-header-quantity {
	margin-right: 13px;
	min-width: 105px;
}

.tribe-editor__ticket__container-header-quantity-label {
	display: block;
	color: #000;
	font-size: 15px;
	line-height: 18px;
	margin-bottom: 12px;
	text-align: center;

	.tribe-editor__ticket--disabled & {
		color: #AEB4BB;
	}
}

.tribe-editor__ticket__container-header-quantity-unlimited {
	display: block;
	color: #AEB4BB;
	font-size: 12px;
	line-height: 14px;
	padding: 2px 0;
	text-align: center;
}
