.tribe-editor__warning-button {
	align-items: center;
	display: flex;
}

.tribe-editor__warning-button-text {
	color: #E7563B;
	font-size: 15px;
	font-weight: bold;
	letter-spacing: 0.38px;
	line-height: 18px;

	.tribe-editor__tickets--loading & {
		color: #AEB4BB;
	}
}

svg.tribe-editor__warning-button-icon {
	fill: #E7563B;
	height: 17px;
	margin-right: 5px;
	width: 17px;

	.tribe-editor__tickets--loading & {
		fill: #AEB4BB;
	}
}
