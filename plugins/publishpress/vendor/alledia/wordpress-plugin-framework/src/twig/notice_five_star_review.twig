<div class="notice notice-success allex-review-notice">
    <p>
        {{ message|format('<b>', '</b>', '<b>', '</b>')|raw }}
    </p>

    <ul>
        <li>
            <a href="{{ links.yes }}" target="_blank">
                <span class="dashicons dashicons-thumbs-up"></span>{{ labels.ok }}
            </a>
        </li>
        <li>
            <a href="{{ links.done }}">
                <span class="dashicons dashicons-smiley"></span>{{ labels.done }}
            </a>
        </li>
        <li>
            <a href="{{ links.no }}">
                <span class="dashicons dashicons-no-alt"></span>{{ labels.no }}
            </a>
        </li>
    </ul>
</div>
